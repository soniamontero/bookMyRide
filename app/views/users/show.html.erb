<%= @user.first_name %>
<%= @user.last_name %>
<%= @user.email %>
<%= @user.location %>
<%= @user.owner %>
<br>
<%= link_to "Edit", edit_user_path(@user) %>
<br>
<% @user.bookings.each do |booking| %>
  <%= booking.date_begin %>
  <%= booking.date_end %>
  <%= booking.ride.name %>
  <%= booking.user_id %>
  <%= link_to "Show", ride_booking_path(booking.ride, booking) %>
<% end %>

<% if @user.avatar.attached? %>
<%= image_tag @user.avatar %>
<% else %>
no
<% end %>


<div class="review-header">
  <div class="review-title">
    <strong><h5>
      <%= review.title %>
    </h5></strong>
  </div>
  <div class="review-user">
    <p>
      <%= review.booking.user_first_name %>
    </p>
    <div class="review-user-avatar avatar">
      <%= cl_image_tag review.booking.user.avatar %>
    </div>
  </div>
</div>

<div class="review-message">
  <p>
    <%= review.message %>
  </p>
</div>


